{
  "name": "@twilio-paste/animation-library",
  "version": "0.3.2",
  "category": "interaction",
  "status": "production",
  "description": "Paste's animation library of choice.",
  "author": "Twilio Inc.",
  "license": "MIT",
  "main": "src/index.tsx",
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "main": "dist/index.js",
    "module": "dist/index.es.js",
    "types": "dist/index.d.ts"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm clean && NODE_ENV=production node build.js && tsc",
    "build:js": "NODE_ENV=development node build.js",
    "build:props": "typedoc --tsconfig ./tsconfig.json --json ./dist/prop-types.json",
    "clean": "rm -rf ./dist && rm -rf node_modules",
    "tsc": "tsc"
  },
  "dependencies": {
    "@react-spring/animated": "9.3.0",
    "@react-spring/core": "9.3.0",
    "@react-spring/rafz": "9.3.0",
    "@react-spring/shared": "9.3.0",
    "@react-spring/types": "9.3.0",
    "@react-spring/web": "9.3.0"
  },
  "peerDependencies": {
    "react": "^16.8.6 || ^17.0.2",
    "react-dom": "^16.8.6 || ^17.0.2"
  },
  "devDependencies": {
    "react": "^16.8.6 || ^17.0.2",
    "react-dom": "^16.8.6 || ^17.0.2"
  }
