{
  "name": "@twilio-paste/icons",
  "version": "5.8.0",
  "description": "Twilio's icon library, ready for use and with accessibility considerations",
  "author": "Twilio Inc.",
  "license": "MIT",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "cjs",
    "esm",
    "json"
  ],
  "scripts": {
    "build": "pnpm clean && NODE_ENV=production node build.js && pnpm tsc && pnpm list-icons && pnpm postbuild",
    "postbuild": "cp -R ./types/* ./cjs && cp -R ./types/* ./esm",
    "build:js": "NODE_ENV=development node build.js && pnpm list-icons && pnpm postbuild",
    "build:props": "typedoc --tsconfig ./tsconfig.json --json ./dist/prop-types.json",
    "clean": "rm -rf ./esm && rm -rf ./cjs && rm -rf ./types",
    "tsc": "tsc",
    "list-icons": "node ./tools/cli.js list-icons",
    "convert-new": "node ./tools/cli.js convert-new",
    "convert": "node ./tools/cli.js convert"
  },
  "peerDependencies": {
    "@twilio-paste/box": "workspace:*",
    "@twilio-paste/design-tokens": "workspace:*",
    "@twilio-paste/style-props": "workspace:*",
    "@twilio-paste/styling-library": "workspace:*",
    "@twilio-paste/theme": "workspace:*",
    "@twilio-paste/types": "workspace:*",
    "@twilio-paste/uid-library": "workspace:*",
    "prop-types": "^15.7.2",
    "react": "^16.8.6 || ^17.0.2",
    "react-dom": "^16.8.6 || ^17.0.2"
  },
  "devDependencies": {
    "@twilio-paste/box": "workspace:*",
    "@twilio-paste/design-tokens": "workspace:*",
    "@twilio-paste/style-props": "workspace:*",
    "@twilio-paste/styling-library": "workspace:*",
    "@twilio-paste/theme": "workspace:*",
    "@twilio-paste/types": "workspace:*",
    "@twilio-paste/uid-library": "workspace:*",
    "prop-types": "^15.7.2",
    "react": "^16.8.6 || ^17.0.2",
    "react-dom": "^16.8.6 || ^17.0.2"
  }
}
